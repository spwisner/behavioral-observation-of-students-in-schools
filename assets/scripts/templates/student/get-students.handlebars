<table class="table student-summary-table table-hover">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>DOB</th>
      <th>School</th>
      <th>Teacher</th>
      <th>Grade</th>
      <th>View Record</th>
      <!-- <th>View Sessions/Reports</th> -->
    </tr>
  </thead>
  <tbody>
    {{#each students as |student|}}
      <tr>
        <td class="display-student-fn">
          {{student.first_name}}
        </td>
        <td class="display-student-ln">
          {{student.last_name}}
        </td>
        <td class="display-student-dob">
          {{student.born_on}}
        </td>
        <td class="display-student-school">
          {{student.school}}
        </td>
        <td class="display-student-teacher">
          {{student.teacher}}
        </td>
        <td class="display-student-grade">
          {{student.grade}}
        </td>
        <td class="display-obs-obs-comment">
          <input class="dashboard-student-record-btn" type="button" value="View Record" data-current-student-id="{{student.id}}">
        </td>
        <!-- <td class="display-obs-obs-comment">
          <input class="dashboard-new-sessions-btn" type="button" value="Create New Session" data-current-student-id="{{student.id}}">
        </td> -->
      </tr>
    {{/each}}
  </tbody>
</table>
<br><br>
<div class="dashboard-container">
  <!-- <input id="dashboard-home-btn" type="button" value="View Records"> -->
  <input id="dashboard-new-session-btn" type="button" value="Create Record">
</div>
